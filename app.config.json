{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "framam-mobile",
    "slug": "framam-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "mycoolredirect",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f7527440-a573-4623-a662-b7b113363ebc"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mustapha100.framammobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.mustapha100.framammobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "iosStatusBar": {
      "backgroundColor": "#C2185B",
      "translucent": false
    },
    "extra": {
      "eas": {
        "projectId": "f7527440-a573-4623-a662-b7b113363ebc"
      }
    },
    "owner": "mustapha100",
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
